
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix expr: <urn:unfinished:lv2:expression:draft:> .

expr:Expression
    a            rdfs:Class ;
    rdfs:comment "A note expression event." .

expr:tuning
    a            lv2:Parameter ;
    rdfs:range   atom:Float ;
    lv2:default  0.0 ;
    rdfs:label   "tuning" ;
    rdfs:comment "Pitch bend, in semitones." .

expr:stereoPanning
    a            lv2:Parameter ;
    rdfs:range   atom:Float ;
    lv2:default  0.0 ;
    lv2:minimum  -1.0 ;
    lv2:maximum  1.0 ;
    rdfs:label   "stereo panning" ;
    rdfs:comment "Stereo panning, from -1.0 (left) to 1.0 (right)" .

expr:supports
    a            rdf:Property ;
    rdfs:range   lv2:Parameter .
